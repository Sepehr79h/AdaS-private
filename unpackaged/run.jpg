# SETUPS=("vgg16-cifar100" "vgg16-cifar10")
# # SETUPS=("vgg16-cifar10")
SETUPS=("resnet34-cifar100" "resnet34-cifar10" "vgg16-cifar100" "vgg16-cifar10")
# OPTIMIZERS=("AdaM" "SLS" "AdaGrad" "RMSProp" "AdaS")
OPTIMIZERS=("SLS")
for setup in "${SETUPS[@]}"; do
        for optimizer in "${OPTIMIZERS[@]}"; do
                if [ -d $setup/$optimizer ]
                then
                        python -m adas train --config $setup/$optimizer/config.yaml --data ../data --output $setup/$optimizer/.output  --checkpoint $setup/$optimizer/.checkpoint
                fi
        done
done
